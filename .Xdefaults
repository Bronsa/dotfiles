URxvt.termName:               rxvt-256color
URxvt.iso14755:               false
URxvt.preeditType:            OverTheSpot
URxvt.imlocale:               en_EN.UTF-8
URxvt.perl-ext-common:        default,matcher,font-size
URxvt.underlineURLs:          true
URxvt.keysym.C-Delete:        perl:matcher
URxvt.background:             #262626
URxvt.transparent:            true
URxvt.shading:                100
URxvt.foreground:             grey80
URxvt.loginShell:             true
URxvt.cursorBlink:            true
URxvt.cursorUnderline:        true
URxvt.saveLines:              32767
URxvt.scrollBar:              false
URxvt.transpscrollbar:        true
URxvt.troughColor:            #000000
URxvt.scrollColor:            #222222
URxvt.cursorColor:            #ffffff
URxvt.pointerColor:           #ffffff
URxvt.scrollstyle:            plain
URxvt.secondaryScroll:        true
URxvt.underlineURLs:          true
URxvt.url-launcher:           firefox
URxvt.keysym.C-Delete:        perl:matcher:last
URxvt.matcher.button:         1
URxvt.buffered:               true
URxvt.showMenu:               false
URxvt.veryBoldFont:           false
URxvt.visualBell:             false
URxvt.hideButtons:            true
URxvt.jumpScroll:             true
URxvt.underlineColor:         #808080
URxvt.secondaryScroll:        true
URxvt.scrollTtyOutput:        false
URxvt.scrollTtyKeypress:      true
URxvt.internalBorder:         1
URxvt.externalBorder:         0
URxvt.searchable-scrollback:  CM-s
URxvt.keysym.C-Return:        \033\015
URxvt.keysym.C-BackSpace:     \033[33~
URxvt.keysym.M-Down:          \033[1;3B
URxvt.keysym.M-Up:            \033[1;3A
URxvt.keysym.M-Left:          \033[1;3D
URxvt.keysym.M-Right:         \033[1;3C
URxvt.font:                   xft:DejaVu Sans Mono-7.5, \
                              xft:VL Gothic-7

URxvt.keysym.CM-Up:     perl:font-size:increase
URxvt.keysym.CM-Down:   perl:font-size:decrease
! URxvt.keysym.C-S-Up:   perl:font-size:incglobal
! URxvt.keysym.C-S-Down: perl:font-size:decglobal


URxvt.cutchars:               ()*,<>[]{}|`""''

URxvt*color1:                 #FF5E5E
URxvt*color9:                 #FF7878
URxvt*color12:                #165eab
URxvt*color4:                 #008AFF

XTerm*Background: black
XTerm*Foreground: white
XTerm*faceName:   DejaVu Sans Mono-40

emacsclient.font:       DejaVu Sans Mono-7.9
