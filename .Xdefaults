URxvt.termName:               rxvt-256color
URxvt.preeditType:            OverTheSpot
URxvt.imlocale:               en_EN.UTF-8
URxvt.perl-ext-common:        default,matcher
URxvt.underlineURLs:          true
URxvt.keysym.C-Delete:        perl:matcher
URxvt.background:             #262626
URxvt*.transparent: true
URxvt*.shading: 20
URxvt.foreground:             grey80
URxvt.loginShell:             true
URxvt.cursorBlink:            true
URxvt.cursorUnderline:        true
URxvt.saveLines:              32767
URxvt.scrollBar:              false
URxvt.transpscrollbar:        true
URxvt.troughColor:            #000000
URxvt.scrollColor:            #222222
URxvt.cursorColor:            #ffffff
URxvt.pointerColor:           #ffffff
URxvt.scrollstyle:            plain
URxvt.secondaryScroll:        true
URxvt.underlineURLs:          true
URxvt.url-launcher:           launch-firefox
URxvt.keysym.C-Delete:        perl:matcher:last
URxvt.matcher.button:         1
URxvt.buffered:               true
URxvt.showMenu:               false
URxvt.veryBoldFont:           false
URxvt.visualBell:             false
URxvt.hideButtons:            true
URxvt.jumpScroll:             true
URxvt.underlineColor:         #808080
URxvt.secondaryScroll:        true
URxvt.scrollTtyOutput:        false
URxvt.scrollTtyKeypress:      true
URxvt.internalBorder:         1
URxvt.externalBorder:         0
URxvt.searchable-scrollback:  CM-s

URxvt.keysym.C-Return:        \033\015
URxvt.keysym.C-BackSpace:     \033[33~
URxvt.font:                   xft:inconsolata:pixelsize=11, \
                              xft:Andale Mono:pixelsize=11, \
                              xft:Verdana:pixelsize=11

! URxvt*font:                            xft:Dejavu Sans Mono:pixelsize=10.5
! URxvt*boldFont:                        xft:Dejavu Sans Mono:pixelsize=10.5
URxvt.cutchars:                        ()*,<>[]{}|`""''
!red
URxvt*color1:              #FF5E5E
URxvt*color9:              #FF7878

!white
URxvt*color7:              #DCDCCC
URxvt*color15:             #FFFFFF
!blue
! URxvt*color4:                 #a9cdeb
URxvt*color12:                #165eab
URxvt*color4:              #008AFF
